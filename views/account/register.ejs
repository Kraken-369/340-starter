<h1><%= title %></h1>
<form action="/account/sign-up" method="post">
  <label for="account_firstname">First Name:</label>
  <input id="account_firstname" name="account_firstname" type="text">
  <label for="account_lastname">Last Name:</label>
  <input id="account_lastname" name="account_lastname" type="text">
  <label for="account_email">Email address:</label>
  <input id="account_email" name="account_email" type="email">
  <label for="account_password">Password:</label>
  <input id="account_password" name="account_password" type="password">
  <button type="submit">Sign Up</button>
</form>

<h2><%- messages() %></h2>